<h1> Holiday requests</h1>
<% @annuals.each do |annual| %>
    <%= image_tag annual.profile.avatar.url(:medium) %>
    <td><%= annual.annualType %></td>
    <td><% if annual.approval == true %>
          Approved
        <% else %>
          Pending
        <%end%>
    </td>
    <br>
    
<% end %>
    